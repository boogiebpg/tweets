<% @posts.each do |post|%>
	<tr>
		<td>
			<%= post.created_at.strftime("%d.%m.%Y %R") %>
		</td>
		<td>
			<%= post.text %>
		</td>
		<td>
			<div class="favorite_add" id="<%= post[:id] %>">
				<% if favorite_tweets.include?(post[:id])%>
					<%= image_tag('star_icon.png', :alt => 'Tweet is already in favorite') %>
				<% else %>
					<%= link_to image_tag('star_icon_inactive.png', :alt => 'Add to favorite'), "#add" %>
				<% end %>
			</div>
		</td>
	</tr>
<% end %>